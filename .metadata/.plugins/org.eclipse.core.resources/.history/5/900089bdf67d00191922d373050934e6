package exercices;

public class ChangementDeVariables {

	public static void main(String[] args) {


		int i = 12; // création de la variable
		String j = new String(); // Création du String vide
		j = j.valueOf(i); // Le sTRing prend la valeur de i
		// Changement de variable int en String
		int k = Integer.valueOf(j).intValue(); // k prend la valeur du String j
		// Changement de variable String en int


		int variable  = 10,variable2 = 20;

		double variablet   = Double.valueOf(variable2) ;
		System.out.println(variablet);
	}

}
